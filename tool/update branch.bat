@echo off
::调用配置bat，设置临时环境变量
call "%~dp0\config"
echo *************************************************************
echo *          批量更新分支：更新当前目录下所有的工程         *  
echo *************************************************************
for /d %%i in (*) do (
	if not %%i==[Filter] (
		if exist "%cd%\%%i\.git\" (
			echo git %%i
			@cd %cd%\%%i && @git pull
			echo ********************************工程更新完成****************************************
		)
	)
)
pause